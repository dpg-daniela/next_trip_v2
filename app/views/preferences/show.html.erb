<div class="page-header">
  <h3>Preference #<%= @preference.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <dt>Country</dt>
      <dd><%= @preference.country %></dd>


      <dt>Destination type</dt>
      <dd><%= @preference.destination_type %></dd>


    </dl>

    <div class="btn-group btn-group-justified">

      <a href="/preferences" class="btn btn-primary">
        Back
      </a>
      <a href="/preferences/<%= @preference.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_preference/<%= @preference.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>

    </div>
  </div>
</div>
